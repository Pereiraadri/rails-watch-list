<h1><%= @list.name %></h1>

<%= link_to "add movie",  new_list_bookmark_path(@list)%>

<ul>
  <% @list.bookmarks.each do |bookmark|%>
      <li>
        <%= bookmark.comment %>
        <%= bookmark.movie.rating %>
        <%= link_to "delete", bookmark_path(bookmark), data: { turbo_method: :delete }%>
      </li>
  <% end %>
</ul>


<%= link_to "lists", lists_path %>
